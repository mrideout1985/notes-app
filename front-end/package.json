{
	"name": "notes-app",
	"version": "0.0.0",
	"author": {
		"name": "Matthew Rideout",
		"url": "https://github.com/mrideout1985"
	},
	"scripts": {
		"build": "tsc -p tsconfig.prod.json && vite build",
		"dev": "vite",
		"format": "prettier . --write --ignore-path .gitignore && git update-index --again",
		"gen": "npx @svgr/cli -d src/components/icons --ignore-existing src/assets/icons --ext=tsx",
		"lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
		"serve": "npm run build && vite preview --port 3000",
		"test": "jest --watchAll --coverage --verbose --detectOpenHandles --forceExit",
		"test:e2e": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress open'",
		"test:e2e:ci": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress run'",
		"typecheck": "tsc --noEmit"
	},
	"lint-staged": {
		"*.{css,md,json}": [
			"prettier --write"
		],
		"*.ts?(x)": [
			"eslint",
			"prettier --write"
		],
		"package.json": "npx sort-package-json"
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	},
	"dependencies": {
		"@emotion/react": "^11.11.0",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.11.16",
		"@mui/lab": "^5.0.0-alpha.125",
		"@mui/material": "^5.13.3",
		"axios": "^1.3.4",
		"open-props": "1.5.9",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "7.44.3",
		"react-router-dom": "6.13.0",
		"react-use": "17.4.0",
		"sass": "1.63.4",
		"sort-package-json": "2.4.1",
		"swr": "^2.1.5",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vite-plugin-svgr": "3.2.0",
		"zustand": "4.3.8"
	},
	"devDependencies": {
		"@babel/core": "^7.22.5",
		"@babel/preset-env": "^7.22.5",
		"@babel/preset-react": "^7.22.5",
		"@commitlint/cli": "17.6.5",
		"@commitlint/config-conventional": "17.6.5",
		"@commitlint/cz-commitlint": "17.5.0",
		"@cypress/vite-dev-server": "5.0.5",
		"@svgr/cli": "8.0.1",
		"@swc/core": "^1.3.64",
		"@swc/jest": "^0.2.26",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^29.5.2",
		"@types/node": "20.3.1",
		"@types/react": "^18.2.12",
		"@types/react-dom": "^18.2.5",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "5.59.11",
		"@typescript-eslint/parser": "5.59.11",
		"@vitejs/plugin-legacy": "4.0.4",
		"@vitejs/plugin-react": "4.0.0",
		"axios-mock-adapter": "^1.21.5",
		"cypress": "12.14.0",
		"eslint": "8.42.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-import-resolver-typescript": "3.5.5",
		"eslint-plugin-cypress": "2.13.3",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"lint-staged": "13.2.2",
		"msw": "^1.2.2",
		"react-test-renderer": "^18.2.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1"
	}
}
